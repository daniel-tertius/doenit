# Production Environment Variables Template
PUBLIC_GOOGLE_AUTH=your-prod-google-auth-client-id
PUBLIC_KEYSTORE_PASSWORD=your-prod-keystore-password
PUBLIC_KEYSTORE_ALIAS=your-prod-keystore-alias
PUBLIC_APP_ID=doenit.app
PUBLIC_APP_NAME=Doenit
PUBLIC_ENVIRONMENT=production

# MongoDB Atlas Configuration (Server-side) - Production
MONGODB_CONNECTION_STRING=mongodb+srv://Prod:YOUR_PROD_PASSWORD@doenitdb.higntyu.mongodb.net/?retryWrites=true&w=majority&appName=DoenitDB
MONGODB_DATABASE_NAME=DoenitDB_Prod
MONGODB_PASSWORD=YOUR_PROD_PASSWORD
MONGODB_USER=Prod

# Client-side: Enable API mode
PUBLIC_VITE_USE_MONGODB_MOBILE=false
PUBLIC_VITE_USE_FIREBASE=true
VITE_ENVIRONMENT=production
