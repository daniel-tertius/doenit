# Development Environment Variables Template
PUBLIC_GOOGLE_AUTH=your-dev-google-auth-client-id
PUBLIC_KEYSTORE_PASSWORD=your-dev-keystore-password
PUBLIC_KEYSTORE_ALIAS=your-dev-keystore-alias
PUBLIC_APP_ID=doenit.app.dev
PUBLIC_APP_NAME=Doenit Dev
PUBLIC_ENVIRONMENT=development

# MongoDB Atlas Configuration (Server-side)
MONGODB_CONNECTION_STRING=mongodb+srv://Dev:YOUR_DEV_PASSWORD@doenitdb.higntyu.mongodb.net/?retryWrites=true&w=majority&appName=DoenitDB
MONGODB_DATABASE_NAME=DoenitDB
MONGODB_PASSWORD=YOUR_DEV_PASSWORD
MONGODB_USER=Dev

# Client-side: Enable API mode
PUBLIC_VITE_USE_MONGODB_MOBILE=false
PUBLIC_VITE_USE_FIREBASE=true
VITE_ENVIRONMENT=development
