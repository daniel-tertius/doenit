rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow users to read/write only their own snapshots
    match /users/{userId}/snapshots/{filename} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
    
    // Deny all other paths
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}